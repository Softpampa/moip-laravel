{
    "name": "softpampa/moip-laravel",
    "description": "Laravel package for Moip SDK PHP",
    "keywords": ["moip", "package", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nícolas Luís Huber",
            "email": "nicolasluishuber@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "softpampa/moip-sdk-php": "dev-develop"
    },
    "autoload": {
        "classmap": [
